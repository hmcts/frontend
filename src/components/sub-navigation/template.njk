<nav class="hmcts-sub-navigation {{ params.classes }}"{% if (params.label) %} aria-label="{{ params.label }}"{% endif %}>
  <div class="hmcts-sub-navigation__container {{ params.containerClasses }}">
    <ul class="hmcts-sub-navigation__list">
      {% for item in params.items %}
        <li class="hmcts-sub-navigation__item">
          <a class="hmcts-sub-navigation__link" {{ 'aria-current=page' if item.active }} href="{{ item.href }}">{{ item.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
</nav>